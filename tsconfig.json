{
  "compilerOptions": {
    "target": "ES2018",
    "module": "commonjs",
    "lib": ["es2018"],
    "declaration": true,
    "strict": true,
    "noImplicitAny": true,
    "strictNullChecks": true,
    "noImplicitThis": true,
    "alwaysStrict": true,
    "noUnusedLocals": false,
    "noUnusedParameters": false,
    "noImplicitReturns": true,
    "noFallthroughCasesInSwitch": false,
    "inlineSourceMap": true,
    "inlineSources": true,
    "experimentalDecorators": true,
    "strictPropertyInitialization": false,
    "typeRoots": ["./node_modules/@types"],
    "baseUrl": "./",
    "paths": {
      "/opt/nodejs/productsLayer": [
        "lambda/products/layers/productsLayer/nodejs/productRepository"
      ],
      "/opt/nodejs/productEventsLayer": [
        "lambda/products/layers/productEventsLayer/nodejs/productEvent"
      ],
      "/opt/nodejs/ordersLayer": [
        "lambda/orders/layers/ordersLayer/nodejs/orderRepository"
      ],
      "/opt/nodejs/ordersApiLayer": [
        "lambda/orders/layers/ordersApiLayer/nodejs/ordersApi"
      ],
      "/opt/nodejs/ordersEventsLayer": [
        "lambda/orders/layers/ordersEventsLayer/nodejs/orderEvents"
      ],
      "/opt/nodejs/ordersEventsRepositoryLayer": [
        "lambda/orders/layers/ordersEventsRepositoryLayer/nodejs/orderEventRepository"
      ],
      "/opt/nodejs/invoiceTransaction": [
        "lambda/invoices/layers/invoiceTransaction/nodejs/invoiceTransaction"
      ],
      "/opt/nodejs/invoiceRepository": [
        "lambda/invoices/layers/invoiceRepository/nodejs/invoiceRepository"
      ],
      "/opt/nodejs/InvoiceWSConection": [
        "lambda/invoices/layers/InvoiceWSConection/nodejs/InvoiceWSConection"
      ]
    }
  },
  "exclude": ["node_modules", "cdk.out"]
}
